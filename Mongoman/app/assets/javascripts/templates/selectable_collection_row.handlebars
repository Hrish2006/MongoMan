{{#with view.content}}
  <td>
    {{view Ember.Checkbox checkedBinding="view.selected" class="database-selector-checkbox"  classNameBindings="selected:selected-row"}}
    <a href="#/documents/{{unbound controller.database}}/{{unbound name}}" class="table-links">{{name}}</a>
  </td>
  <td>{{stats.count}}</td>
  <td>{{truncate stats.avgObjSize}}  bytes</td>
  <td>{{humanize stats.size}}</td>
  <td>{{stats.nindexes}}</td>
{{/with}}

